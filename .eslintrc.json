{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "max-len": [2, 100],
    "@typescript-eslint/no-explicit-any": "error",
    "no-console": "off",
    "linebreak-style": ["error", "windows"],
    "quotes": [
       "error",
       "double"
     ]
    }
  }
